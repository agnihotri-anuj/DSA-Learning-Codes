
#include <iostream>
#include <vector>
#include<algorithm>
using namespace std;
int main()
{
    vector<int> arr1{2, 4, 8, 10};
    vector<int> arr2{1, 3, 5, 7, 9};
    vector<int> arr3;
    for (int i = 0; i < arr1.size(); i++)
    {
        arr3.push_back(arr1[i]);
    }
    for (int i = 0; i < arr2.size(); i++)
    {
        arr3.push_back(arr2[i]);
    }
    sort(arr3.begin(), arr3.end());
    for (int i = 0; i < arr3.size(); i++)
    {
        cout << arr3[i] << " ";
    }

    return 0;
}
