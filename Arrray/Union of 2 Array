
#include <iostream>
#include <vector>
#include <limits.h>
using namespace std;

// union of 2 array
int main()
{
    int arr[]={2,4,6,8};
    int sizea=4;
    int brr[]={3,5,7,9};
    int sizeb=4;

    vector<int> ans;

    for(int i=0;i<sizea;i++)
    {
        ans.push_back(arr[i]);
    }

    for(int i=0;i<sizea;i++)
    {
        ans.push_back(brr[i]);
    }

    for(int i=0;i<ans.size();i++)
    {
        cout<<ans[i]<<" ";
    }
return 0;

}
